<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Section2Q8">

    <TextView
        android:id="@+id/textView34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:fontFamily="@font/baloo2"
        android:text="Question 8/8"
        android:textColor="@color/black"
        android:textSize="11sp"
        android:layout_marginStart="30dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgback" />

    <TextView
        android:id="@+id/textView67"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:fontFamily="@font/nats_regular"
        android:text="SECTION 2"
        android:textColor="@color/black"
        android:textSize="26sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/imgback"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="23dp"
        android:backgroundTint="@color/white"
        app:layout_constraintBottom_toBottomOf="@+id/textView67"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView67"
        app:srcCompat="@drawable/ic_baseline_arrow_back_black"
        app:tint="@color/black" />

    <!--    <ProgressBar-->
    <!--        android:id="@+id/progressBar"-->
    <!--        android:layout_width="311dp"-->
    <!--        android:layout_height="4dp"-->
    <!--        android:progress="37"-->
    <!--        app:layout_constraintBottom_toBottomOf="parent"-->
    <!--        app:layout_constraintEnd_toEndOf="parent"-->
    <!--        app:layout_constraintStart_toStartOf="parent"-->
    <!--        app:layout_constraintTop_toTopOf="parent"-->
    <!--        app:layout_constraintVertical_bias="0.193"-->
    <!--        style="?android:attr/progressBarStyleHorizontal"-->
    <!--        android:progressDrawable="@drawable/custom_progress_bg2"-->
    <!--        />-->

    <ImageView
        android:id="@+id/imageView27"
        android:layout_width="0dp"
        android:layout_height="587dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="30dp"
        android:scaleType="fitXY"
        android:src="@drawable/multiple_choice_background"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView34" />

    <LinearLayout
        android:id="@+id/radioGroup"
        android:layout_width="0dp"
        android:layout_height="454dp"
        android:layout_marginTop="10dp"
        android:layout_alignTop="@+id/textView77"
        android:layout_centerHorizontal="true"
        android:layout_marginHorizontal="15dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="@+id/imageView27"
        app:layout_constraintStart_toStartOf="@+id/imageView27"
        app:layout_constraintTop_toBottomOf="@+id/textView77">

        <CheckBox
            android:id="@+id/dia"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/radiobtn_off"
            android:fontFamily="@font/nats_regular"
            android:text="Diabetes"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <!--            Hypothyroidism-->

        <CheckBox
            android:id="@+id/hyperthy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/radiobtn_off"
            android:fontFamily="@font/nats_regular"
            android:text="Hyperthyroidism"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <CheckBox
            android:id="@+id/hypothy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/radiobtn_off"
            android:fontFamily="@font/nats_regular"
            android:text="Hypothyroidism"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <!--            <CheckBox-->
        <!--                android:id="@+id/hypothy"-->
        <!--                android:layout_width="match_parent"-->
        <!--                android:layout_height="wrap_content"-->
        <!--                android:layout_marginTop="10dp"-->
        <!--                android:background="@drawable/radiobtn_off"-->
        <!--                android:fontFamily="@font/nats_regular"-->
        <!--                android:text="Hypothyroidism"-->
        <!--                android:textAlignment="center"-->
        <!--                android:textColor="@color/black"-->
        <!--                android:textSize="18sp" />-->

        <CheckBox
            android:id="@+id/hyperten"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/radiobtn_off"
            android:fontFamily="@font/nats_regular"
            android:text="Hypertension"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <CheckBox
            android:id="@+id/pcod"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/radiobtn_off"
            android:fontFamily="@font/nats_regular"
            android:text="PCOD/PCOS"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <CheckBox
            android:id="@+id/fattyl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/radiobtn_off"
            android:fontFamily="@font/nats_regular"
            android:text="Fatty liver"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <CheckBox
            android:id="@+id/lactose"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/radiobtn_off"
            android:fontFamily="@font/nats_regular"
            android:text="Lactose intolerance"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <EditText
            android:id="@+id/oth"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:background="@drawable/radiobtn_off"
            android:fontFamily="@font/nats_regular"
            android:hint="Other ___________"
            android:textAlignment="center"
            android:textColorHint="@color/black"
            android:textSize="18sp" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView77"
        android:text="Family history of"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="25dp"
        android:fontFamily="@font/baloo2"
        android:textColor="@color/black"
        android:textSize="11sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="@+id/imageView27"
        app:layout_constraintStart_toStartOf="@+id/imageView27"
        app:layout_constraintTop_toTopOf="@+id/imageView27" />

    <ImageView
        android:id="@+id/imageView38"
        android:src="@mipmap/blback"
        android:backgroundTint="#9873FF"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        app:layout_constraintBottom_toBottomOf="@+id/backbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.198"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/backbtn"/>

    <TextView
        android:id="@+id/backbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:clickable="true"
        android:fontFamily="@font/nats_regular"
        android:text="Back"
        android:textColor="#9873FF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/nextbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.276"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup" />

    <Button
        android:id="@+id/nextbtn"
        android:text="Next"
        android:layout_width="117dp"
        android:layout_height="50dp"
        android:backgroundTint="#9873FF"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:fontFamily="@font/nats_regular"
        android:paddingTop="0dp"
        android:paddingBottom="5dp"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="18sp"
        app:cornerRadius="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.744"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup" />

</androidx.constraintlayout.widget.ConstraintLayout>